{
    "author": "navikt",
    "description": "Diverse utils for Sykdom i familien teamet",
    "homepage": "https://navikt.github.io/sif-common-utils",
    "license": "MIT",
    "main": "lib/index.js",
    "name": "@navikt/sif-common-utils",
    "types": "lib/index.d.js",
    "version": "3.9.0",
    "bugs": {
        "url": "https://github.com/navikt/sif-common-utils/issues"
    },
    "dependencies": {
        "iso8601-duration": "^1.3.0",
        "lodash.uniq": "^4.5.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@types/lodash.uniq": "^4.5.7",
        "@types/node": "^17.0.38",
        "@types/react": "^17.0.41",
        "@types/react-dom": "^17.0.14",
        "dayjs": "^1.11.3",
        "eslint-config-custom": "*",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.1",
        "typedoc": "^0.22.17",
        "web-vitals": "^2.1.4"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "peerDependencies": {
        "dayjs": "^1.11.x"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-common-utils.git"
    },
    "scripts": {
        "build": "npm-run-all clean-lib compile-lib-script",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-w": "tsc -p tsconfig.json --watch",
        "dev": "yarn compile-lib-script-w",
        "test": "react-scripts test --watchAll=false",
        "test-w": "react-scripts test",
        "test-c": "npm-run-all jest-clear-cache test-coverage",
        "test-cw": "npm-run-all jest-clear-cache test-coverage-watch",
        "test-coverage": "react-scripts test a --coverage --watchAll=false",
        "test-coverage-watch": "react-scripts test a --coverage --watchAll=true",
        "jest-clear-cache": "jest --clearCache"
    },
    "private": false,
    "snyk": true
}
