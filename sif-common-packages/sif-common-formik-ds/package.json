    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-common-formik-ds.git"
    },
    "scripts": {
        "build": "npm-run-all compile-lib-script copy-files",
        "build-storybook": "build-storybook -s public",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-w": "tsc -p tsconfig.json --watch",
        "copy-files": "npm-run-all copy-css-files-to-lib copy-sass-files-to-lib",
        "copy-css-files-to-lib": "copyfiles -u 1 \"src/**/*.css\" lib",
        "copy-sass-files-to-lib": "copyfiles -u 1 \"src/**/*.scss\" lib",
        "dev": "yarn compile-lib-script-w",
        "storybook": "start-storybook -p 6006 -s public",
        "test": "react-scripts test --watchAll=false --env=jsdom",
        "test-w": "react-scripts test",
        "test-c": "npm-run-all jest-clear-cache test-coverage",
        "test-cw": "npm-run-all jest-clear-cache test-coverage-watch",
        "test-coverage": "react-scripts test a --coverage --watchAll=false",
        "test-coverage-watch": "react-scripts test a --coverage --watchAll=true",
        "jest-clear-cache": "jest --clearCache"
    },
    "private": false
}
