{
    "author": "NAV",
    "description": "Monorepo for felleskode og selvbetjeningsdialoger i Sykdom i familien",
    "license": "MIT",
    "name": "@navikt/sif-brukerdialog",
    "repository": "https://github.com/navikt/sif-brukerdialog",
    "version": "0.0.1",
    "bugs": {
        "url": "https://github.com/navikt/sif-brukerdialog/issues"
    },
    "devDependencies": {
        "@changesets/cli": "^2.24.3",
        "@navikt/ds-tailwind": "^0.2.9",
        "@snyk/protect": "^1.987.0",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/manager-webpack5": "^6.5.10",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "esbuild": "^0.15.5",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.22.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "jest-environment-jsdom": "^28.1.3",
        "jsdom": "^20.0.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "npm-watch": "^0.11.0",
        "postcss-cli": "^10.0.0",
        "postcss-loader": "^7.0.1",
        "postcss-nested": "^5.0.6",
        "prettier": "^2.7.1",
        "react-day-picker": "^7.4.10",
        "react-scripts": "5.0.1",
        "ts-jest": "^28.0.7",
        "turbo": "^1.4.3",
        "typescript": "^4.7.4"
    },
    "lint-staged": {
        "apps/**/*.{ts,tsx}": [
            "eslint --fix"
        ],
        "packages/**/*.{ts,tsx}": [
            "eslint --fix"
        ]
    },
    "scripts": {
        "build": "turbo run build",
        "build-package": "turbo run build-package",
        "bp": "turbo run build-package",
        "build-app": "turbo run build-app",
        "build-sif-ettersending": "turbo run build-app --filter=@navikt/sif-ettersending",
        "build-deling-omsorgsdager": "turbo run build-app --filter=@navikt/omsorgsdager-deling-dialog",
        "build-ekstra-omsorgsdager": "turbo run build-app --filter=ekstra-omsorgsdager-andre-forelder-ikke-tilsyn",
        "ba": "turbo run build-app",
        "clean": "turbo run clean",
        "clean-all": "turbo run clean && rm -rf node_modules",
        "dev": "turbo run dev --no-cache --parallel --continue",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "husky install",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "test-watch": "turbo run test-watch",
        "test-coverage": "turbo run test-coverage"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "overrides": {
        "react-scripts": {
            "nth-check": ">=2.0.1"
        },
        "glob-parent": ">=5.1.2"
    },
    "private": true,
    "snyk": true,
    "resolutions": {
        "@types/react": "18.0.17",
        "@types/react-dom": "17.0.14"
    },
    "dependencies": {
        "classnames": "^2.3.1",
        "prop-types": "^15.8.1"
    }
}
