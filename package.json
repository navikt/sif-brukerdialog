{
    "author": "NAV",
    "description": "Monorepo for felleskode og selvbetjeningsdialoger i Sykdom i familien",
    "license": "MIT",
    "name": "@navikt/sif-brukerdialog",
    "repository": "https://github.com/navikt/sif-brukerdialog",
    "version": "0.0.1",
    "bugs": {
        "url": "https://github.com/navikt/sif-brukerdialog/issues"
    },
    "devDependencies": {
        "@changesets/cli": "2.26.0",
        "@navikt/ds-tailwind": "2.0.17",
        "@snyk/protect": "1.1087.0",
        "@storybook/builder-webpack5": "6.5.15",
        "@storybook/manager-webpack5": "6.5.15",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/react-hooks": "8.0.1",
        "@testing-library/user-event": "14.4.3",
        "esbuild": "0.17.3",
        "esbuild-jest": "0.5.0",
        "eslint": "8.32.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "jest-environment-jsdom": "29.3.1",
        "jsdom": "21.0.0",
        "lint-staged": "13.1.0",
        "npm-run-all": "4.1.5",
        "npm-watch": "0.11.0",
        "postcss-cli": "10.1.0",
        "postcss-loader": "7.0.2",
        "postcss-nested": "6.0.0",
        "prettier": "2.8.3",
        "react-day-picker": "8.4.1",
        "react-scripts": "5.0.1",
        "react-syntax-highlighter": "15.5.0",
        "turbo": "1.7.0",
        "typescript": "4.9.4",
        "yarn-audit-fix": "9.3.8"
    },
    "lint-staged": {
        "apps/**/*.{ts,tsx}": [
            "eslint --fix"
        ],
        "packages/**/*.{ts,tsx}": [
            "eslint --fix"
        ]
    },
    "scripts": {
        "build": "turbo run build",
        "build-package": "turbo run build-package",
        "bp": "turbo run build-package",
        "build-app": "turbo run build-app",
        "build-sif-ettersending": "turbo run build-app --filter=@navikt/sif-ettersending",
        "e2e-sif-ettersending": "turbo run ci --filter=@navikt/sif-ettersending",
        "build-omsorgspengesoknad": "turbo run build-app --filter=@navikt/omsorgspengesoknad",
        "e2e-omsorgspengesoknad": "turbo run ci --filter=@navikt/omsorgspengesoknad",
        "build-endringsmelding-pleiepenger": "turbo run build-app --filter=@navikt/endringsmelding-pleiepenger",
        "e2e-endringsmelding-pleiepenger": "turbo run ci --filter=@navikt/endringsmelding-pleiepenger",
        "ba": "turbo run build-app",
        "clean": "turbo run clean",
        "clean-all": "turbo run clean && rm -rf node_modules",
        "dev": "turbo run dev --no-cache --parallel --continue",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "husky install",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "test-watch": "turbo run test-watch",
        "test-coverage": "turbo run test-coverage"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "overrides": {
        "react-scripts": {
            "nth-check": ">=2.0.1"
        },
        "glob-parent": ">=5.1.2"
    },
    "private": true,
    "snyk": true,
    "dependencies": {
        "classnames": "2.3.2",
        "prop-types": "15.8.1"
    }
}
