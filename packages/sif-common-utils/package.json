{
    "author": "navikt",
    "description": "Diverse utils for Sykdom i familien teamet",
    "main": "lib/index.js",
    "name": "@navikt/sif-common-utils",
    "types": "lib/index.d.js",
    "version": "3.40.1",
    "private": true,
    "dependencies": {
        "dayjs": "1.11.10",
        "iso8601-duration": "2.1.1",
        "lodash": "4.17.21",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@babel/core": "7.22.20",
        "@babel/preset-env": "7.22.20",
        "@babel/preset-react": "7.22.15",
        "@types/jest": "29.5.5",
        "@types/node": "20.6.3",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@types/uuid": "9.0.4",
        "config": "workspace:*",
        "eslint-config-custom": "*",
        "npm-run-all": "4.1.5",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "5.0.1"
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all compile-lib-script",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "dev": "yarn compile-lib-script-watch",
        "test": "react-scripts test --watchAll=false --env=jsdom",
        "test-watch": "react-scripts test",
        "test-coverage": "react-scripts test a --coverage --watchAll=false",
        "test-coverage-watch": "react-scripts test a --coverage --watchAll=true"
    }
}
