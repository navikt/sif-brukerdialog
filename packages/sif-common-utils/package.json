    },
    "devDependencies": {
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@types/lodash.uniq": "^4.5.7",
        "@types/node": "^18.6.5",
        "@types/react": "17.0.34",
        "@types/react-dom": "^17.0.14",
        "dayjs": "^1.11.4",
        "eslint-config-custom": "*",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.1",
        "typedoc": "^0.23.10",
        "web-vitals": "^2.1.4"
