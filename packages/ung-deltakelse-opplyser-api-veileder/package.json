{
    "name": "@navikt/ung-deltakelse-opplyser-api-veileder",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "private": true,
    "type": "module",
    "version": "0.1.29",
    "devDependencies": {
        "@hey-api/openapi-ts": "0.86.7",
        "@sif/eslint-config": "workspace:*",
        "config": "workspace:*",
        "typescript": "5.9.3",
        "vitest": "4.0.4",
        "zod": "4.1.12"
    },
    "peerDependencies": {
        "dayjs": "*",
        "uuid": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-brukerdialog.git"
    },
    "scripts": {
        "fix-regexp": "node scripts/fix-generated-regex.mjs",
