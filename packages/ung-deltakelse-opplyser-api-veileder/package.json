        "lint:eslint": "eslint './src/veileder/client/*.gen.ts'",
        "lint:fix": "eslint --quiet --fix './src/veileder/client/*.gen.ts'",
        "lint:tsc": "tsc --noEmit"
    },
    "dependencies": {
        "@hey-api/client-axios": "0.9.1",
        "@hey-api/client-fetch": "0.13.1"
