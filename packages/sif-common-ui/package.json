{
    "name": "@navikt/sif-common-ui",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "version": "0.0.1",
    "private": true,
    "devDependencies": {
        "@storybook/addon-a11y": "7.4.3",
        "@storybook/addon-actions": "7.4.3",
        "@storybook/addon-essentials": "7.4.3",
        "@storybook/addon-interactions": "7.4.3",
        "@storybook/addon-links": "7.4.3",
        "@storybook/node-logger": "7.4.3",
        "@storybook/preset-create-react-app": "7.4.3",
        "@storybook/react": "7.4.3",
        "@storybook/react-webpack5": "7.4.3",
        "@storybook/testing-library": "0.2.1",
        "copyfiles": "2.4.1",
        "npm-run-all": "4.1.5",
        "react-scripts": "5.0.1",
        "storybook": "7.4.3",
        "storybook-addon-sass-postcss": "0.1.3"
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all compile-lib-script copy-files",
