{
    "name": "@navikt/ung-deltakelse-opplyser-api-deltaker",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "private": true,
    "type": "module",
    "version": "0.3.13",
    "devDependencies": {
        "@hey-api/openapi-ts": "0.92.3",
        "@sif/eslint-config": "workspace:*",
        "axios": "1.13.5",
        "config": "workspace:*",
        "postcss": "8.5.6",
        "typescript": "5.9.3",
        "vitest": "4.0.18",
        "zod": "4.3.6"
    },
    "peerDependencies": {
        "dayjs": "*",
        "uuid": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-brukerdialog.git"
    },
    "scripts": {
        "fix-regexp": "node scripts/fix-generated-regex.mjs",
        "gen-types:fixed": "yarn gen-types && yarn fix-regexp",
        "gen-types": "openapi-ts -f openapi-ts.deltaker.config.ts",
        "codegen:dev": "CODEGEN_ENV=dev yarn gen-types:fixed",
        "codegen:prod": "CODEGEN_ENV=prod yarn gen-types:fixed",
        "lint:eslint": "eslint './src/deltaker/client/*.gen.ts'",
        "lint:fix": "eslint --quiet --fix './src/deltaker/client/*.gen.ts'",
        "lint:tsc": "tsc --noEmit"
    },
    "dependencies": {
        "@hey-api/client-axios": "0.9.1",
        "@hey-api/client-fetch": "0.13.1",
        "@navikt/sif-common-api": "workspace:*"
    }
}
