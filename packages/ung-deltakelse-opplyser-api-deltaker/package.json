{
    "name": "@navikt/ung-deltakelse-opplyser-api-deltaker",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "private": true,
    "type": "module",
    "version": "0.1.17",
    "devDependencies": {
        "@hey-api/openapi-ts": "0.83.1",
        "@sif/eslint-config": "workspace:*",
        "config": "workspace:*",
        "typescript": "5.9.2",
        "vitest": "3.2.4",
        "zod": "4.1.7"
    },
    "peerDependencies": {
        "dayjs": "*",
        "uuid": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-brukerdialog.git"
    },
    "scripts": {
        "fix-regexp": "node scripts/fix-generated-regex.mjs",
        "gen-types:fixed": "yarn gen-types && yarn fix-regexp",
        "gen-types": "openapi-ts -f openapi-ts.deltaker.config.ts",
        "codegen": "yarn gen-types:fixed",
        "lint:eslint": "eslint './src/**/*.gen.ts' --ignore-pattern './src/generated/**/client/**' --ignore-pattern './src/generated/**/core/**'",
        "lint:fix": "eslint --quiet --fix './src/**/*.gen.ts' --ignore-pattern './src/generated/**/client/**' --ignore-pattern './src/generated/**/core/**'",
        "lint:tsc": "tsc --noEmit"
    },
    "dependencies": {
        "@hey-api/client-axios": "0.9.1",
        "@hey-api/client-fetch": "0.13.1",
        "@navikt/sif-common-api": "workspace:*"
    }
}
