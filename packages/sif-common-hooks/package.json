{
    "name": "@navikt/sif-common-hooks",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "version": "0.1.3",
    "private": true,
    "dependencies": {
        "react": "18.2.0",
        "typescript": "5.3.3"
    },
    "devDependencies": {
        "copyfiles": "2.4.1",
        "npm-run-all": "4.1.5"
    },
    "scripts": {
        "build-package": "npm-run-all compile-lib-script copy-files",
        "build": "npm-run-all build-package",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "compile-lib-script": "tsc -p tsconfig.json",
        "copy-files": "copyfiles -u 1 \"src/**/*.css\" lib",
        "lint": "eslint ./src --ext ts,tsx"
    }
}
