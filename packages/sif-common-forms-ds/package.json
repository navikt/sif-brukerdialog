{
    "author": "@navikt",
    "description": "Fellesskjema for sykdom i familien",
    "homepage": "https://navikt.github.io/sif-brukerdialog",
    "license": "ISC",
    "main": "lib/index.js",
    "name": "@navikt/sif-common-forms-ds",
    "types": "lib/index.d.js",
    "version": "1.3.0",
    "bugs": {
        "url": "https://github.com/navikt/sif-brukerdialog/issues"
    },
    "dependencies": {},
    "devDependencies": {
        "@formatjs/intl-pluralrules": "^5.0.3",
        "@navikt/sif-common-core-ds": "*",
        "@navikt/sif-common-formik-ds": "*",
        "@navikt/sif-common-utils": "*",
        "@navikt/ds-css": "^1.0.2",
        "@navikt/ds-react": "^1.0.2",
        "@storybook/addon-a11y": "^6.5.10",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/node-logger": "^6.5.10",
        "@storybook/react": "^6.5.10",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^18.7.10",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/react-responsive": "^8.0.5",
        "axios": "^0.27.2",
        "copyfiles": "^2.4.1",
        "core-js": "^3.24.1",
        "dayjs": "^1.11.5",
        "flat": "^5.0.2",
        "formik": "^2.2.9",
        "gh-pages": "^4.0.0",
        "history": "^5.3.0",
        "intl": "^1.2.5",
        "jsdom": "^20.0.0",
        "less-plugin-npm-import": "^2.1.0",
        "react": "^18.2.0",
        "react-aria-menubutton": "^7.0.3",
        "react-collapse": "^5.1.1",
        "react-day-picker": "^8.1.0",
        "react-dom": "^18.2.0",
        "react-intl": "6.0.8",
        "react-modal": "^3.15.1",
        "react-responsive": "^8.2.0",
        "react-router-dom": "^5.3.3",
        "react-scripts": "^5.0.1",
        "rimraf": "^3.0.2",
        "webpack": "5.74.0"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!(@navikt)).*\\.js$"
        ]
    },
    "peerDependencies": {
        "@navikt/ds-css": "1",
        "@navikt/ds-react": "1",
        "@navikt/sif-common-core": ">= 11.3.0 <=12.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/sif-brukerdialog.git"
    },
    "watch": {
        "copy-files": {
            "patterns": [
                "src"
            ],
            "extensions": "css,scss,less",
            "runOnChangeOnly": true
        }
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all clean-lib compile-lib-script copy-files",
        "build-storybook": "build-storybook",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rm -rf lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "copy-files": "copyfiles -u 1 \"src/**/*.less\" lib/ && copyfiles -u 1 \"src/**/assets/**/*\" lib/ && copyfiles -u 1 \"src/**/*.json\" lib/",
        "deploy": "gh-pages -d build",
        "dev": "yarn watch-styles & yarn compile-lib-script-watch",
        "watch-styles": "npm-watch copy-files ",
        "storybook": "start-storybook -p 6006",
        "test": "react-scripts test --watchAll=false --env=jsdom",
        "test-watch": "react-scripts test"
    },
    "private": false
}
