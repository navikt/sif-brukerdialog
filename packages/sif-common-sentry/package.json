{
    "author": "navikt",
    "description": "Sentry utils used in sif-common applications",
    "homepage": "https://navikt.github.io/sif-common-sentry",
    "license": "MIT",
    "main": "lib/index.js",
    "name": "@navikt/sif-common-sentry",
    "types": "lib/index.d.js",
    "version": "0.9.3",
    "bugs": {
        "url": "https://github.com/navikt/sif-common-sentry/issues"
    },
    "dependencies": {
        "@sentry/browser": "7.40.0",
        "@sentry/react": "7.40.0",
        "@sentry/types": "7.40.0"
    },
    "devDependencies": {
        "@types/node": "18.14.4",
        "axios": "1.3.4",
        "copyfiles": "2.4.1",
        "gh-pages": "5.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "5.0.1",
        "rimraf": "4.1.3"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "package.json",
        "/lib/index.d.ts",
        "/lib/index.ts"
    ],
    "peerDependencies": {
        "axios": ">= 0.27.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-common-sentry.git"
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all compile-lib-script",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "dev": "yarn compile-lib-script-watch",
        "test": "react-scripts test --watchAll=false --env=jsdom",
        "test-watch": "react-scripts test"
    },
    "private": false
}
