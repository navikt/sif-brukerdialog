    "devDependencies": {
        "@types/node": "^17.0.38",
        "axios": "^0.27.2",
        "copyfiles": "^2.4.1",
        "gh-pages": "^4.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.1",
