{
    "name": "@navikt/k9-brukerdialog-prosessering-api",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "private": true,
    "type": "module",
    "version": "0.0.13",
    "devDependencies": {
        "@hey-api/openapi-ts": "0.78.3",
        "@sif/eslint-config": "workspace:*",
        "config": "workspace:*",
        "typescript": "5.8.3",
        "vitest": "3.2.4",
        "zod": "3.25.76"
    },
    "peerDependencies": {
        "dayjs": "*",
        "uuid": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-brukerdialog.git"
    },
    "scripts": {
        "lint:eslint": "eslint \"./src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint --quiet --fix './src/**/*.{js,ts,tsx}'",
        "lint:tsc": "tsc --noEmit",
        "gen-types": "openapi-ts --file openapi-ts.config.ts",
        "gen-types:omsorgspenger": "openapi-ts --file openapi-ts.config-omsorgspenger.ts",
        "gen-types:ungdomsytelse": "openapi-ts --file openapi-ts.config-ungdomsytelse.ts",
        "gen-types:all": "yarn gen-types:omsorgspenger &&  yarn gen-types:ungdomsytelse && yarn gen-types",
        "fix-regex": "node scripts/fix-generated-regex.mjs",
        "gen-types:all:fixed": "yarn gen-types:all && yarn fix-regex"
    },
    "dependencies": {
        "@hey-api/client-axios": "0.9.1",
        "@hey-api/client-fetch": "0.13.1"
    }
}
