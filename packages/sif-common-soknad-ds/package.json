        "@testing-library/user-event": "14.5.1",
        "@types/jest": "29.5.5",
        "@types/node": "20.7.0",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@types/react-modal": "3.16.0",
        "axios": "1.5.0",
        "copyfiles": "2.4.1",
        "dayjs": "1.11.10",
        "formik": "2.4.5",
        "gh-pages": "6.0.0",
        "intl": "1.2.5",
        "io-ts-types": "0.5.19",
        "monocle-ts": "2.3.13",
        "newtype-ts": "0.3.5",
        "react": "18.2.0",
        "react-day-picker": "8.8.2",
        "react-dom": "18.2.0",
        "react-intl": "6.4.7",
        "react-router-dom": "6.16.0",
        "react-scripts": "5.0.1",
        "rimraf": "5.0.4",
        "sass": "1.68.0",
        "sass-loader": "13.3.2",
        "storybook": "7.4.5",
        "uuid": "9.0.1",
        "webpack": "5.88.2"
    },
    "watch": {
        "copy-files": {
            "patterns": [
                "src"
            ],
            "extensions": "css,scss",
            "runOnChangeOnly": true
        }
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all clean-lib compile-lib-script copy-files",
        "build-storybook": "build-storybook -s public",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "copy-files-watch": "npm-watch copy-files",
        "copy-files": "npm-run-all copy-sass-files copy-css-files copy-asset-files copy-json-files",
        "copy-sass-files": "copyfiles -u 1 \"src/**/*.scss\" lib/",
        "copy-css-files": "copyfiles -u 1 \"src/**/*.css\" lib/",
        "copy-asset-files": "copyfiles -u 1 \"src/**/assets/**/*\" lib/",
        "copy-json-files": "copyfiles -u 1 \"src/**/*.json\" lib/",
        "deploy": "gh-pages -d build",
        "dev": "yarn compile-lib-script-watch & yarn copy-files-watch",
        "lint": "eslint ./src --ext ts,tsx",
        "storybook": "storybook dev -p 6006 -s public",
        "test": "react-scripts test --watchAll=false --passWithNoTests"
    }
}
