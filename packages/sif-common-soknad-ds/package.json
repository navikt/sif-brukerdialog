    },
    "devDependencies": {
        "@devexperts/remote-data-ts": "^2.1.1",
        "@formatjs/intl-pluralrules": "^5.0.3",
        "@navikt/appstatus-react": "^3.1.0",
        "@navikt/ds-css": "^1.0.2",
        "@navikt/ds-react": "^1.0.2",
        "@navikt/sif-common-core-ds": "*",
        "@navikt/sif-common-formik-ds": "*",
        "@navikt/sif-common-sentry": "0.4.0",
        "@storybook/addon-a11y": "^6.5.10",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/node-logger": "^6.5.10",
        "@storybook/react": "^6.5.10",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^18.7.6",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/react-modal": "^3.13.1",
        "axios": "^0.27.2",
        "copyfiles": "^2.4.1",
        "dayjs": "^1.11.5",
        "formik": "^2.2.9",
        "gh-pages": "^3.2.3",
        "intl": "^1.2.5",
        "less-plugin-npm-import": "^2.1.0",
        "react": "^18.2.0",
        "react-day-picker": "^8.1.0",
        "react-intl": "^6.0.5",
        "react-scripts": "5.0.1",
        "rimraf": "^3.0.2",
        "ulid": "^2.3.0"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "peerDependencies": {
        "@devexperts/remote-data-ts": "^2.0.x",
        "@formatjs/intl-pluralrules": "^4.1.x",
        "@navikt/appstatus-react": "^3.0.0",
        "@navikt/ds-css": "^1.0.2",
        "@navikt/ds-react": "^1.0.2",
        "@navikt/sif-common-core": "12.x",
        "@navikt/sif-common-formik-ds": "*",
        "@navikt/sif-common-sentry": "*",
        "dayjs": "^1.10.x"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-brukerdialog.git"
    },
    "watch": {
        "copy-files": {
            "patterns": [
                "src"
            ],
            "extensions": "css,scss,less",
            "runOnChangeOnly": true
        }
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all clean-lib compile-lib-script copy-files",
        "build-storybook": "build-storybook -s public",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "copy-files-watch": "npm-watch copy-files",
        "copy-files": "npm-run-all copy-sass-files copy-less-files copy-css-files copy-asset-files copy-json-files",
        "copy-sass-files": "copyfiles -u 1 \"src/**/*.scss\" lib/",
        "copy-less-files": "copyfiles -u 1 \"src/**/*.less\" lib/",
        "copy-css-files": "copyfiles -u 1 \"src/**/*.css\" lib/",
        "copy-asset-files": "copyfiles -u 1 \"src/**/assets/**/*\" lib/",
        "copy-json-files": "copyfiles -u 1 \"src/**/*.json\" lib/",
        "deploy": "gh-pages -d build",
        "dev": "yarn compile-lib-script-watch & yarn copy-files-watch",
        "storybook": "start-storybook -p 6006 -s public",
        "test": "react-scripts test --watchAll=false --passWithNoTests"
    },
    "private": false
}
