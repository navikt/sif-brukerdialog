    "bugs": {
        "url": "https://github.com/navikt/sif-brukerdialog/issues"
    },
    "devDependencies": {
        "@devexperts/remote-data-ts": "2.1.1",
        "@formatjs/intl-pluralrules": "5.1.8",
        "@navikt/appstatus-react-ds": "*",
        "@navikt/ds-css": "2.1.5",
        "@navikt/ds-react": "2.1.5",
        "@navikt/sif-common-core-ds": "*",
        "@navikt/sif-common-formik-ds": "*",
        "@navikt/sif-common-sentry": "0.6.1",
        "@storybook/addon-a11y": "6.5.16",
        "@storybook/addon-actions": "6.5.16",
        "@storybook/addon-essentials": "6.5.16",
        "@storybook/addon-interactions": "6.5.16",
        "@storybook/addon-links": "6.5.16",
        "@storybook/node-logger": "6.5.16",
        "@storybook/react": "6.5.16",
        "@storybook/testing-library": "0.0.13",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/node": "18.11.18",
        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "@types/react-modal": "3.13.1",
        "axios": "1.3.1",
        "copyfiles": "2.4.1",
        "dayjs": "1.11.7",
        "formik": "2.2.9",
        "gh-pages": "4.0.0",
        "intl": "1.2.5",
        "react": "18.2.0",
        "react-day-picker": "8.5.1",
        "react-intl": "6.2.7",
        "react-scripts": "5.0.1",
        "rimraf": "4.1.2",
        "ulid": "2.3.0"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "peerDependencies": {
        "@devexperts/remote-data-ts": "2.0.x",
        "@formatjs/intl-pluralrules": "4.1.x",
        "@navikt/appstatus-react-ds": "1.5.8",
        "@navikt/ds-css": "2.1.5",
        "@navikt/ds-react": "2.1.5",
        "@navikt/sif-common-formik-ds": "*",
        "@navikt/sif-common-sentry": "*",
        "dayjs": "1.10.x"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-brukerdialog.git"
    },
    "watch": {
        "copy-files": {
            "patterns": [
                "src"
            ],
            "extensions": "css,scss",
            "runOnChangeOnly": true
        }
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all clean-lib compile-lib-script copy-files",
        "build-storybook": "build-storybook -s public",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "copy-files-watch": "npm-watch copy-files",
        "copy-files": "npm-run-all copy-sass-files copy-css-files copy-asset-files copy-json-files",
        "copy-sass-files": "copyfiles -u 1 \"src/**/*.scss\" lib/",
        "copy-css-files": "copyfiles -u 1 \"src/**/*.css\" lib/",
        "copy-asset-files": "copyfiles -u 1 \"src/**/assets/**/*\" lib/",
        "copy-json-files": "copyfiles -u 1 \"src/**/*.json\" lib/",
        "deploy": "gh-pages -d build",
        "dev": "yarn compile-lib-script-watch & yarn copy-files-watch",
        "storybook": "start-storybook -p 6006 -s public",
        "test": "react-scripts test --watchAll=false --passWithNoTests"
    },
    "private": false
}
