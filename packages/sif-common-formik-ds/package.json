        "@storybook/test-runner": "0.16.0",
        "@storybook/testing-library": "0.2.2",
        "@types/jest": "29.5.10",
        "@types/node": "20.10.3",
        "@types/react": "18.2.41",
        "@types/react-dom": "18.2.17",
        "@types/uuid": "9.0.7",
        "@vitejs/plugin-react": "4.2.0",
        "config": "workspace:*",
        "copyfiles": "2.4.1",
        "date-fns": "2.30.0",
        "dayjs": "1.11.10",
        "formik": "2.4.5",
        "jest": "29.7.0",
