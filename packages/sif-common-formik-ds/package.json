{
    "author": "navikt",
    "homepage": "https://navikt.github.io/sif-common-formik-ds",
    "license": "MIT",
    "main": "lib/index.js",
    "name": "@navikt/sif-common-formik-ds",
    "types": "lib/index.d.js",
    "version": "0.1.0",
    "bugs": {
        "url": "https://github.com/navikt/sif-common-formik-ds/issues"
    },
    "dependencies": {
        "i18n-iso-countries": "^7.5.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@formatjs/intl-pluralrules": "^5.0.2",
        "@navikt/ds-css": "^0.18.19",
        "@navikt/ds-datepicker": "^3.1.0-beta.1",
        "@navikt/ds-react": "^0.19.15",
        "@navikt/fnrvalidator": "^1.3.0",
        "@storybook/addon-a11y": "^6.5.9",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-interactions": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/node-logger": "^6.5.9",
        "@storybook/react": "^6.5.9",
        "@storybook/testing-library": "^0.0.13",
        "@types/node": "^18.0.0",
        "@types/react": "17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/uuid": "^8.3.4",
        "copyfiles": "^2.4.1",
        "dayjs": "^1.11.3",
        "formik": "^2.2.9",
        "less-loader": "^11.0.0",
        "mini-css-extract-plugin": "^2.6.1",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-day-picker": "^7.4.10",
        "react-dom": "^17.0.2",
        "react-intl": "6.0.4",
        "react-responsive": "^9.0.0-beta.10",
        "react-scripts": "5.0.1",
        "rimraf": "^3.0.2",
        "sass": "^1.52.3",
        "storybook-formik": "^2.4.0"
    },
    "directories": {
        "lib": "lib"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!(@navikt)).*\\.js$"
        ]
    },
    "overrides": {
        "react-refresh": "0.11.0"
    },
    "peerDependencies": {
        "@navikt/ds-css": "0.18.x",
        "@navikt/ds-datepicker": "3.0.0",
        "@navikt/ds-react": "0.18.x",
        "formik": "2.2.x",
        "react": "17.0.x",
        "react-dom": "17.0.x",
        "react-intl": "6.0.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/sif-common-formik-ds.git"
    },
    "scripts": {
        "build-package": "npm-run-all compile-lib-script copy-files",
        "build-storybook": "build-storybook -s public",
        "clean": "rm -rf node_modules lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig.json",
        "compile-lib-script-watch": "tsc -p tsconfig.json --watch",
        "copy-css-files-to-lib": "copyfiles -u 1 \"src/**/*.css\" lib",
        "copy-files": "npm-run-all copy-css-files-to-lib copy-sass-files-to-lib",
        "copy-sass-files-to-lib": "copyfiles -u 1 \"src/**/*.scss\" lib",
        "dev": "yarn watch-styles & yarn compile-lib-script-watch",
        "storybook": "start-storybook -p 6006 -s public",
        "test": "react-scripts test --watchAll=false --env=jsdom",
        "test-coverage": "react-scripts test a --coverage --watchAll=false",
        "test-coverage-watch": "react-scripts test a --coverage --watchAll=true",
        "test-watch": "react-scripts test",
        "watch-styles": "npm-watch copy-files"
    },
    "watch": {
        "copy-files": {
            "extensions": "css,scss,less",
            "patterns": [
                "src"
            ],
            "runOnChangeOnly": true
        }
    },
    "private": false
}
