    "devDependencies": {
        "@navikt/ds-css": "^1.0.2",
        "@navikt/ds-react": "^1.0.2",
        "@types/node": "^18.7.6",
        "@types/react": "18.0.17",
        "@types/react-dom": "^18.0.6",
        "copyfiles": "^2.4.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
