        "sass-loader": "^13.x"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/appstatus-react-ds.git"
    },
    "scripts": {
        "build": "npm-run-all build-package",
        "build-package": "npm-run-all compile-lib-script copy-files",
