        "babel-core": "7.0.0-bridge.0"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!(@navikt)).*\\.js$"
        ]
    },
    "snyk": true,
    "eslintConfig": {
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    }
