    "scripts": {
        "build-storybook": "storybook build",
        "build": "vite build --base=/familie/sykdom-i-familien/soknad/opplaringspenger",
        "build-demo": "npm run build-demo:vite && npm run build-demo:copy-service-worker",
        "build-demo:vite": "vite build --base=/ --outDir=demo",
        "build-demo:copy-service-worker": "cp ./mockServiceWorker.js ./demo/mockServiceWorker.js",
        "clean": "rm -rf node_modules .dist",
        "dev-api": "PORT=8089 node ./mock/mock-api-server.cjs",
        "dev": "node ./dev-server.cjs",
        "demo": "node ./demo-server.cjs",
        "lint:eslint": "eslint \"./src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint --quiet --fix './src/**/*.{js,ts,tsx}'",
        "lint:tsc": "tsc --noEmit",
        "playwright-test": "playwright test",
        "start-e2e-server": "node ./e2e/server/server.cjs",
        "storybook": "storybook dev -p 6006",
        "test": "vitest run"
    },
    "eslintConfig": {
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "msw": {
        "workerDirectory": [
            "dist"
        ]
