        "transformIgnorePatterns": [
            "node_modules/(?!(@navikt)).*\\.js$"
        ]
    },
    "eslintConfig": {
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    }
