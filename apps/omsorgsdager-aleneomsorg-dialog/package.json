    "version": "2.10.7",
    "private": true,
    "scripts": {
        "build-app": "node src/build/scripts/production-build.js",
        "build": "npm-run-all build-app",
        "dev-api": "PORT=8089 node ./server/api-mock.js",
        "start-dev": "npm-run-all -p rm:dist dev-api start-no-decorator",
        "rm:dist": "rimraf dist/",
        "start-api-mock": "PORT=8089 node ./server/api-mock.js",
        "start-express": "node ./server.js",
        "snd": "node ./src/build/scripts/start-dev-no-decorator.js",
        "start-no-decorator": "node ./src/build/scripts/start-dev-no-decorator.js",
        "start-server-local": "npm-run-all -s rm:dist build -p dev-api server-test:local",
        "start": "node ./src/build/scripts/start-dev.js",
        "test": "react-scripts test --watchAll=false --env=jsdom",
        "test-watch": "react-scripts test --watchAll=true --env=jsdom",
        "lint": "eslint ./src --ext ts,tsx",
        "ci": "start-server-and-test 'yarn start-e2e-server' http://localhost:8080/familie/sykdom-i-familien/soknad/omsorgsdager-aleneomsorg/alive 'yarn cypress-run'",
        "ci-server": "server-test http://localhost:8080/familie/sykdom-i-familien/soknad/omsorgsdager-aleneomsorg/alive",
        "cypress-open": "cypress open --project $PWD/e2e",
        "cypress-run": "cypress run --project $PWD/e2e",
        "start-e2e-server": "node ./server-e2e.js"
    },
    "dependencies": {
        "@navikt/ds-css": "5.6.3",
